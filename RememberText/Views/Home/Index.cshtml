@model List<RememberText.Domain.Entities.IpAddress>

@{
    ViewData["Title"] = "Home Page";
}

<h4>Current Ip Address: @ViewBag.CacheIpAddress</h4>
<h4>Second Ip Address: @ViewBag.ipadress</h4>

<div id="ProjectsBox">
    <partial name="Partial/_Projects" data="@Model" />
</div>

<!-- Modal text -->
<div class="modal fade" id="RT_ModalForProject" tabindex="-1" role="dialog" aria-labelledby="RT_ModalTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="RT_ModalTitle">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div id="RT_ModalBody" class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
